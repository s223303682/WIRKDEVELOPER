@model IEnumerable<WIRKDEVELOPER.Models.DayHospital>
@{

    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div class=" col-10 text-right">
    <a asp-controller="Admin" asp-action="CreateDayHospital" style="width:100%"></a>
</div>
@if (Model.Count() > 0)
{
    <table id="BookingsTable" class=" table table-bordered table-striped table-white">
        <thead>
            <tr>
                <th>Name</th>
                <th>Province</th>
                <th>city</th>
                <th>subrbub</th>
                <th>Contact</th>
                <th>Email address</th>
                <th>Practice Manager</th>
                <th>Phurhase Manager Email</th>
                <th> Update</th>
                <th>Delete</th>


            </tr>
        </thead>
        <tbody>
            @foreach (var DayHospital in Model)
            {

                <tr>
                    <td>@DayHospital.Name</td>
                    <td>@DayHospital.province</td>
                    <td>@DayHospital.City</td>
                    <td>@DayHospital.Surbub</td>
                    <td>@DayHospital.ContactNumber</td>
                    <td>@DayHospital.emailAddress</td>
                    <td>@DayHospital.PracticeManager</td>
                    <td>@DayHospital.PhurshaseManagerEmail</td>
                    
                    <td>
                        <div class="">
                            <a asp-controller="Admin" asp-action="updateDayHospital" asp-route-ID="@DayHospital.DayHospitalID" class=" btn btn-blue-300 mx-1">Update</a>
                        </div>
                    </td>
                    <td>
                        <a asp-controller="Admin" asp-action="DeleteDayHospital" asp-route-ID="@DayHospital.DayHospitalID" class=" btn btn-blue-300 mx-1">Delete</a>
                    </td>

                </tr>



            }

        </tbody>
    </table>
}
else
{
    <p>NO Records has been done</p>
}